    function saveCupponDetail() {

        var obj = new Object();

        obj.WayBillBookNo = getControlValueN("txtCpnWaybillBook");
        obj.WayBillSerialNo = getControlValueN("txtCpnWaybillSerial");
        obj.CouponBookNo = getControlValueN("txtCpnBook");
        obj.CouponSerialNo = getControlValueN("txtCpnSerial");
        obj.StationFrom = getControlValueN("txtCpnStationFrom");
        obj.StationTo = getControlValueN("txtCpnStationTo");
        obj.Fare = getControlValueN("txtCpnFare");
        obj.TotalAmount = getControlValueN("ttlamt");
        $.ajax({
            type: "POST",
            url: '/WayBill/SaveWayBillCouponDetails',
            data: JSON.stringify(obj),
            contentType: "application/json; charset=utf-8",
            dataType: "json",
            success: function (data) {
                document.getElementById("btnClose").click();
                showWaybllDetail(data);
            },
            error: OnError
        });
    }

