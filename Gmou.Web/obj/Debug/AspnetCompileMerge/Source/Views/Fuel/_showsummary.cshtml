
@model Gmou.DomainModelEntities.AllSalesModel
<div class="col-md-12 marT20">
    <div class="box box-success">
        <div class="boxHeading">
            <b>Petrol</b>
            <a href="" class="pull-right hide" id="ancAddNewStore" title="Add a Store"><i class="fa fa-plus"></i> Add Related Account</a>
        </div>
        <div class="box-body">
            <table id="tblAccount" style="width:100%" class="tbl20 alignROdd">
                <tbody>
                    <tr>
                        <td>
                            Start Meter Reading
                        </td>
                        <td>
                            <input type="text" class="" id="txtPatrolStart" />
                        </td>
                        <td>
                            End Meter Reading
                        </td>
                        <td>
                            <input type="text" class="" id="txtPatrolEnd" />
                        </td>
                        <td>
                            <input type="button" id="btnpetrolshow" value="Show" onclick="GetPetrolCash(1);" />
                        </td>
                    </tr>
                    <tr>

                        <td>
                            Cash
                        </td>
                        <td>
                            <input type="text" class="" id="txtPatrolCash" />
                        </td>
                        <td>Qty</td>
                        <td>
                            <input type="text" class="" id="txtPatrolCashQty" />
                        </td>
                        <td>Amount</td>
                        <td>
                            <input type="text" class="" id="txtPatrolCashAmount" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Owner sale
                        </td>
                        <td>
                            <input type="text" id="txtPatrolOwner" />
                        </td>
                        <td>Qty</td>
                        <td>
                            <input type="text" class="" id="txtPatrolOwnerQty" value=@Model.PetrolQuantity />
                        </td>
                        <td>Amount</td>
                        <td>
                            <input type="text" class="" id="txtPatrolOwnerAmount" value=@Model.PetrolAmount />
                        </td>
                    </tr>


                    <tr>
                        <td>
                            Staff Sale
                        </td>
                        <td>
                            <input type="text" class="" id="txtPatrolStaff" />
                        </td>
                        <td>Qty</td>
                        <td>
                            <input type="text" class="" id="txtPatrolStaffQty" value=@Model.SPetrolQuantity />
                        </td>
                        <td>Amount</td>
                        <td>
                            <input type="text" class="" id="txtPatrolStaffAmount" value=@Model.SPetrolAmount />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            Other Sale
                        </td>
                        <td>
                            <input type="text" id="txtPatrolOtherSale" class="" />
                        </td>
                        <td>Qty</td>
                        <td>
                            <input type="text" class="" id="txtPatrolOtherQty" value=@Model.OPetrolQuantity />
                        </td>
                        <td>Amount</td>
                        <td>
                            <input type="text" class="" id="txtPatrolOtherAmount" value=@Model.OPetrolAmount />
                        </td>
                    </tr>

                    <tr>
                        <td>
                            Water Dip
                        </td>
                        <td>
                            <input type="text" class="" id="txtPatrolWaterDip" />
                        </td>
                        <td>
                            Other Summary
                        </td>
                        <td>
                            <input type="text" id="txtPatrolOther" />
                        </td>
                    </tr>


                </tbody>
            </table>
        </div>
    </div>
</div>
