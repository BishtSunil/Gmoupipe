@model Gmou.Web.Models.LoginViewModel

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
    <link href="~/Content/themes/Gmou/style.css" rel="stylesheet" />
</head>
<body>
@using (Html.BeginForm("CheckLogin", "Home"))
{
    <div class="login">
        <div class="login-triangle"></div>

        <h2 class="login-header">Log in</h2>
        <form class="login-container">
            <div>Hello Div hello class</div>
            <p>@Html.TextBoxFor(m=>m.Username)</p>
            <p>@Html.TextBoxFor(m=>m.Password)</p>
            <span>Demo</span>
            <div class="dropdown">
                <button class="btn btn-primary dropdown-toggle" type="button" data-toggle="dropdown">
                    Dropdown Example
                    <span class="caret"></span>
                </button>
                <ul class="dropdown-menu">
                    <li><a href="#">HTML</a></li>
                    <li><a href="#">CSS</a></li>
                    <li><a href="#">JavaScript</a></li>
                </ul>
            </div>
            <p><input type="submit" value="Log in"></p>
        </form>
    </div>
}

    
    
 
    <script src='http://cdnjs.cloudflare.com/ajax/libs/jquery/2.1.3/jquery.min.js'></script>



</body>
</html>
