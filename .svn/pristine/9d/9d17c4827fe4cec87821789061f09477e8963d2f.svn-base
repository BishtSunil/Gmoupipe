            <script type="text/javascript">

                $(function () {
                    $(".datepicker").datepicker({ dateFormat: 'dd-mm-yy' });

                });
                function DateTimeFunction(id) {
                    var dt = getControlValueN(id);
                    if (dt != null) {
                        var day = dt.split("-");
                        var d = new Date(day[2], day[1], day[0]);
                        return d.toLocaleDateString();
                    }
                }
                $(document).ready(function () {
                    $('#txtOwnerName').prop('disabled', true);
                    $('#ddlBusNumber').change(function () {
                        alert("Hello");
                    });
                    $('.custom-combobox-input').focusout(function () {
                        GetBusOwnerName();
                    });
                    $('#txtVSerialNo').focusout(function () {
                        CheckValidSerialno();
                    });
                    $('#txtVSerialNo').focusin(function () {
                        HideErrorMessage();
                    });

                    $("#checkbox").change(function () {
                        if (this.checked) {
                            alert("a");
                        }
                    });
                });
                function CheckValidSerialno() {
                    $.ajax({
                        type: "GET",
                        url: "/WayBill/ValidateWayBillSerialNumber",
                        data: { waybillserialno: $('#txtVSerialNo').val(), waybillbookno: $('#txtVbookNo').val(), busID: $('#ddlBusNumber').val() },
                        datatype: "json",
                        traditional: true,
                        success: function (data) {
                            if (data != null) {
                              
                                if (data == true) {
                                    HideErrorMessage();

                                }
                                else {
                                    ShowErrorMessage();
                                }


                            }
                        }
                    });

                }
                function HideErrorMessage() {
                    $('#txtVSerialNo').css('background-color', 'white')
                    $('#lblmissing').css("color", "red");
                    $('#lblmissing').hide();
                    $('#lblmissing').text("")



                }
                function ShowErrorMessage() {
                    $('#lblmissing').show();
                    $('#txtVSerialNo').css('background-color', 'red')
                    $('#lblmissing').css("color", "red");
                    $('#lblmissing').text("Serial number is missing.Please Give the Reason")
                }
                function GetBusOwnerName() {
                    $.ajax({
                        type: "GET",
                        url: "/WayBill/GetBusOwnerName",
                        data: { busID: $('#ddlBusNumber').val() },
                        datatype: "json",
                        traditional: true,
                        success: function (data) {
                            if (data != null) {
                           
                                $('#txtVbookNo').val(data.WayBillBookNo);
                                $('#txtOwnerName').val(data.OwnerName);


                            }
                            else {
                                $('#txtOwnerName').prop('disabled', false);
                                $('#txtVbookNo').val("");
                                $('#txtOwnerName').val("");
                            }
                        }
                    });

                }
                function calcTotalAmount() {

                    var calcAmount = parseFloat(getControlValue("txtPrice")) * parseFloat(getControlValue("txtFuelQuantity"));;

                    setControlValue("txtAmount", calcAmount);
                }

                function saveVivraniDetails() {

                    var valid = validateTextBoxes('busInformation');
                    if (valid == true) {

                        var obj = new Object();


                        obj.vivraniSerialNumber = @ViewBag.serialnumber
                        obj.VechileNumber = getControlValueN("ddlBusNumber");
                        obj.OwnerName = getControlValueN("ddlownerName");
                        obj.ServiceName = getControlValueN("ddlDipoo");
                        obj.Detail = getControlValueN("txtDetail");
                        obj.DemoDate = getControlValueN("txtDate");
                        obj.WayBillNumber = getControlValueN("txtVbookNo");
                        obj.WayBillSerialNumber = getControlValueN("txtVSerialNo");
                        obj.TicketNumber = getControlValueN("txtTktBookNo");
                        obj.TicketSerailNumber = getControlValueN("txtTicketSerialNo");
                        obj.ServiceTo = getControlValueN("txtArrival");
                        obj.ServiceFrom = getControlValueN("txtDearture");
                        obj.Amount = getControlValueN("txtMoney");
                        obj.WayBillDate = DateTimeFunction("txtDate");
                        obj.TotalAmount = getControlValueN("ttlamt");

                        var fuelobj = new Object();


                        fuelobj.FuelDate = getControlValueN("txtFuelDate");
                        if (getControlValueN("txtOthers") != "") {
                            fuelobj.Fueltype = getControlValueN("txtOthers");
                        }
                        else {
                            fuelobj.Fueltype = getControlValueN("ddlFuelType");
                        }

                        fuelobj.ChitNumber = getControlValueN("txtChttNo");
                        fuelobj.Quantity = getControlValueN("txtFuelQuantity");
                        fuelobj.FuelStation = getControlValueN("ddlPumpName");
                        fuelobj.FuelPrice = getControlValueN("txtPrice");
                        fuelobj.Amount = getControlValueN("txtAmount");
                        fuelobj.FuelTypeOther = getControlValueN("txtOthers");
                        obj.vivranifuel = fuelobj;


                        // obj.Reason = getControlValueN("txtReason");
                        $.ajax({
                            type: "POST",
                            url: '/WayBill/AddVivraniDeatil',
                            data: JSON.stringify(obj),

                            contentType: "application/json; charset=utf-8",
                            dataType: "json",
                            success: function (data) {
                                alert("Done");

                                setControlValue("ttlamt", data[0].TotalAmount);
                                $("#ttlamtttxt").removeClass("hide")
                                if (data) {
                                    var len = data.length;
                                    var txt = "";
                                    if (len > 0) {

                                        for (var i = 0; i < len; i++) {


                                            txt += "<tr><td>" + data[i].vivraniSerialNumber + "</td><td>" + data[i].VechileNumber + "</td>"
                                                    + "<td id='wbBkNo'>" + data[i].WayBillNumber + "</td>" + "<td id='wbSrno'>" + data[i].WayBillSerialNumber + "</td>"
                                                      + "<td>" + data[i].ServiceFrom + "</td>" + "<td>" + data[i].ServiceTo + "</td>"
                                                  + "<td>" + data[i].Amount + "</td><td><a href='#' data-toggle='modal' data-target='#myModal'  onclick= 'getAllWaybillCouponDetail("  + data[i].WayBillNumber + ',' + data[i].WayBillSerialNumber  + ")'>Add Cuppon</a></td></tr>";


                                        }

                                        if (txt != "") {
                                            $("#EmployeesTable").append(txt);

                                        }
                                    }
                                }

                            },
                            error: OnError
                        });
                    }
                }
                function showOtherExpences() {
                    var url = "/WayBill/AddOtherExpences";
                    window.location.href = url;
                }


                function getAllWaybillCouponDetail(wbBkNo, wbSrno)
                {
                    alert(wbBkNo);
                    //var wbBkNo = getControlValueN("wbBkNo");
                    //var wbSrno = getControlValueN("wbSrno");
                    setControlValue("txtWaybillBook", wbBkNo);
                    setControlValue("txtWaybillSerial", wbSrno);
                    $.ajax({
                        type: "GET",
                        url: "/WayBill/GetAllWayBillCouponDetails",
                        data: { waybillbookno: wbBkNo ,waybillserialno: wbSrno},
                        datatype: "json",
                        traditional: true,
                        success: function (data) {
                            if (data != null) {
                                getCouponDetail(data);


                            }
                            else {
                                
                            }
                        }
                    });
                   
                }

                function saveCupponDetail()
                {
                    var obj = new Object();
                    obj.WayBillBookNo = getControlValueN("txtWaybillBook");
                    obj.WayBillSerialNo = getControlValueN("txtWaybillSerial");
                    obj.CouponBookNo = getControlValueN("txtBook");
                    obj.CouponSerialNo = getControlValueN("txtSerial");
                    obj.StationFrom = getControlValueN("txtStationFrom");
                    obj.StationTo = getControlValueN("txtStationTo");
                    obj.Fare = getControlValueN("txtFare");
                    obj.Distance = getControlValueN("txtDistance");
                    obj.TotalAmount = getControlValueN("ttlCPNamt");
                    $.ajax({
                        type: "POST",
                        url: '/WayBill/SaveWayBillCouponDetails',
                        data: JSON.stringify(obj),
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {
                            clearAllTextBoxes("pnlCoupon");
                            getCouponDetail(data);
                        },
                        error: OnError
                    });
                }

                function getCouponDetail(data)
                {
                    
                    setControlValue("ttlamt", data[0].TotalAmount);
                    $("#ttlamtttxt").removeClass("hide")
                    if (data) {

                        var len = data.length;
                        var txt = "";
                        if (len > 0) {
                            for (var i = 0; i < len; i++) {
                                txt += "<tr><td>" + data[i].CouponBookNo + "</td><td>" + data[i].CouponSerialNo + "</td>"
                                        + "<td>" + data[i].StationFrom + "</td>" + "<td>" + data[i].StationTo + "</td>"
                                          + "<td>" + data[i].Fare + "</td>" + "<td>" + data[i].Distance + "</td>"
                                      + "<td><a href='javascript:void(0)' onclick= editBuses(" + '"' + + '"' + ")><img src='../Images/edit.png' style='height:20px' /></a></td>"

                      + "<td><a href='javascript:void(0)' onclick= deleteBus(" + '"' + + '"' + ")><img src='../Images/delete.png' style='height:20px' /></a></td></td></tr>";


                            }

                            if (txt != "") {
                                $("#CupponTable").append(txt);

                            }
                        }
                    }
                }
                function saveWarrent()
                {
                    var obj = new Object();
                    obj.From = getControlValueN("txtBusFrom")
                    obj.BusTo = getControlValueN("txtBusTo")
                    obj.BusNumber = getControlValueN("ddlBusNumber")
                    obj.CupponCode = getControlValueN("txtWarrentNo")
                    $.ajax({
                        type: "POST",
                        url: '/Admin/Save',
                        data: JSON.stringify(obj),
                        contentType: "application/json; charset=utf-8",
                        dataType: "json",
                        success: function (data) {
                            alert("done");
                        },
                        error: OnError
                    });
                }


                function abc() {
                    alert("close");

                    // $(this).removeData('bs.modal');

                }
            </script>
